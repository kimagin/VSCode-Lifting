  "terminal.integrated.fontWeight": "normal",
  "zenMode.fullScreen": true,
  "zenMode.hideLineNumbers": false,
  "git.autofetch": true,
  "files.autoSave": "onFocusChange",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.closeEmptyGroups": true,
  "window.closeWhenEmpty": true,
  "workbench.editor.empty.hint": "text",
  "workbench.reduceMotion": "on",
  "workbench.tree.renderIndentGuides": "onHover",
  "workbench.tree.indent": 8,
  "workbench.tree.expandMode": "singleClick",
  "editor.peekWidgetDefaultFocus": "editor",
  "editor.quickSuggestionsDelay": 50,
  "editor.renderLineHighlight": "all",
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.renderWhitespace": "none",
  "editor.guides.indentation": false,
  "editor.bracketPairColorization.enabled": false,
  "editor.codeLens": true,
  "editor.guides.highlightActiveBracketPair": false,

  "editor.suggest.insertMode": "replace",

  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.minimap.enabled": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.largeFileOptimizations": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "prettier.bracketSameLine": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "editor.scrollbar.horizontalScrollbarSize": 4,
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.tabSize": 2,
  "[css]": {},
  "[javascript]": {},
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.endOfLine": "auto",
  "prettier.singleAttributePerLine": true,
  "prettier.documentSelectors": ["**/*.astro"],
  "better-comments.highlightPlainText": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.bracketPairsHorizontal": false,
  "editor.autoIndent": "advanced",
  "editor.formatOnSave": true,
  "editor.tabCompletion": "on",
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.suggest.showDeprecated": false,
  "tailwindCSS.classAttributes": ["class", "className"],
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.suggest.localityBonus": true,
  "editor.suggest.preview": true,
  "editor.suggestSelection": "first",
  "javascript.suggest.completeFunctionCalls": true,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "typescript.updateImportsOnFileMove.enabled": "always",

  // Code Actions
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "explicit",
    "source.organizeImports": "never"
  },

  // Vim Controls

  "vim.leader": "<Space>",
  "vim.sneak": true,
  "vim.highlightedyank.enable": true,
  "vim.autoindent": true,
  "vim.foldfix": true,
  "vim.enableNeovim": true,
  "vim.useSystemClipboard": true,
  "vim.easymotion": true,
  "vim.hlsearch": false,
  "vim.history": 10000,
  "vim.mouseSelectionGoesIntoVisualMode": true,
  "vim.replaceWithRegister": true,
  "vim.sneakReplacesF": true,
  "vim.easymotionKeys": "hklyuiopnmwertzxcvbasdgjf",
  "vim.easymotionDimColor": "#000000",
  "vim.easymotionIncSearchForegroundColor": "#00ffff",
  "vim.easymotionMarkerBackgroundColor": "#000000",
  "vim.easymotionMarkerForegroundColorOneChar": "#97E0FA",
  "vim.easymotionMarkerFontWeight": "normal",
  "vim.easymotionMarkerForegroundColorTwoCharFirst": "#FA8D19",
  "vim.changeWordIncludesWhitespace": false,

  "vim.handleKeys": {
    "<C-a>": false,
    "<c-d>": false,
    "<C-c>": false,
    "<C-v>": false,
    "<C-x>": false,
    "<C-o>": false
  },

  "vim.searchHighlightColor": "#d4c3c3",
  "vim.searchMatchTextColor": "#d4c3c3",
  "vim.searchHighlightTextColor": "#000",
  "vim.highlightedyank.color": "rgb(13, 185, 215)",
  "vim.highlightedyank.textColor": "#000",

  "vim.visualModeKeyBindingsNonRecursive": [
    //paste without override
    {
      "before": ["p"],
      "after": ["p", "g", "v", "y"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.showCommands"]
    }
  ],

  // "extensions.experimental.affinity": {
  //   "vscodevim.vim": 1
  // },

  "vim.insertModeKeyBindingsNonRecursive": [
    { "before": ["j", "j"], "after": ["<Esc>"] }
    // {
    //   "before": ["<C-p>"],
    //   "after": ["<C-n>"]
    // }
  ],

  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["c"],
      "after": ["\"", "_", "c"]
    },
    {
      "before": ["<leader>", "n"],
      "commands": [":bprevious"]
    },
    {
      "before": ["<leader>", "b"],
      "commands": [":bnext"]
    },
    {
      "before": ["<leader>", "\\"],
      "commands": [":vsplit"]
    },
    {
      "before": ["<leader>", "-"],
      "commands": [":split"]
    },
    {
      "before": ["<leader>", "q", "q"],
      "commands": [":wq!"]
    },
    {
      "before": ["<leader>", "w", "w"],
      "commands": [":w!"]
    },
    {
      "before": ["<leader>", "r"],
      "commands": [
        "workbench.action.toggleAuxiliaryBar",
        "workbench.action.focusActiveEditorGroup"
      ]
    },
    {
      "before": ["s"],
      "after": ["leader", "leader", "2", "s"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": ["livegrep.search"]
    },
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "o"],
      "commands": ["gitProjectManager.openProject"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "e", "e"],
      "commands": [
        {
          "command": "extension.multiCommand.execute",
          "args": {
            "sequence": [
              "workbench.action.terminal.new",
              {
                "command": "workbench.action.terminal.sendSequence",
                "args": {
                  "text": "editor && exit\n"
                }
              }
            ]
          }
        }
      ]
    },
    {
      "before": ["<leader>", "c", "n"],
      "commands": [
        {
          "command": "extension.multiCommand.execute",
          "args": {
            "sequence": [
              "workbench.action.terminal.new",
              {
                "command": "workbench.action.terminal.sendSequence",
                "args": {
                  "text": "note && exit\n"
                }
              }
            ]
          }
        }
      ]
    },
    {
      "before": ["<leader>", "g", "g"],
      "commands": [
        {
          "command": "extension.multiCommand.execute",
          "args": {
            "sequence": [
              "workbench.action.terminal.new",
              {
                "command": "workbench.action.terminal.sendSequence",
                "args": {
                  "text": "lazygit && exit\n"
                }
              }
            ]
          }
        }
      ]
    },
    {
      "before": ["<leader>", "f", "g"],
      "commands": [
        {
          "command": "extension.multiCommand.execute",
          "args": {
            "sequence": [
              "workbench.action.terminal.new",
              {
                "command": "workbench.action.terminal.sendSequence",
                "args": {
                  "text": "sf_home && exit\n"
                }
              }
            ]
          }
        }
      ]
    },

    {
      "before": ["<leader>", "g", "o"],
      "commands": [
        {
          "command": "extension.multiCommand.execute",
          "args": {
            "sequence": [
              "workbench.action.terminal.new",
              {
                "command": "workbench.action.terminal.sendSequence",
                "args": {
                  "text": "repos && exit\n"
                }
              }
            ]
          }
        }
      ]
    },

    {
      "before": ["<leader>", "f", "n"],
      "commands": [
        {
          "command": "extension.multiCommand.execute",
          "args": {
            "sequence": [
              "workbench.action.terminal.new",
              {
                "command": "workbench.action.terminal.sendSequence",
                "args": {
                  "text": "sf_notes && exit\n"
                }
              }
            ]
          }
        }
      ]
    },
    {
      "before": ["<leader>", "s", "n"],
      "commands": [
        {
          "command": "extension.multiCommand.execute",
          "args": {
            "sequence": [
              "workbench.action.terminal.new",
              {
                "command": "workbench.action.terminal.sendSequence",
                "args": {
                  "text": "sw_notes && exit\n"
                }
              }
            ]
          }
        }
      ]
    },
    {
      "before": ["<leader>", "s", "c"],
      "commands": [
        {
          "command": "extension.multiCommand.execute",
          "args": {
            "sequence": [
              "workbench.action.terminal.new",
              {
                "command": "workbench.action.terminal.sendSequence",
                "args": {
                  "text": "sw_cwd\n"
                }
              }
            ]
          }
        }
      ]
    },
    {
      "before": ["<leader>", "f", "f"],
      "commands": [
        {
          "command": "extension.multiCommand.execute",
          "args": {
            "sequence": [
              "workbench.action.terminal.new",
              {
                "command": "workbench.action.terminal.sendSequence",
                "args": {
                  "text": "sf_cwd\n"
                }
              }
            ]
          }
        }
      ]
    }
  ],

