  "css.lint.unknownAtRules": "ignore",
  "editor.fontFamily": "JetBrainsMono Nerd Font Mono",
  "editor.fontWeight": "400",
  "editor.suggestFontSize": 16,
  "editor.fontLigatures": true,
  "git.enableSmartCommit": true,
  "files.associations": {
    "*.vue": "vue",
    "*.js": "javascript",
    "*.astro": "astro",
    "*.glsl": "glsl",
    "*.ts": "typescript",
    "*.json": "json"
