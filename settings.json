  "workbench.colorCustomizations": {
    // Global Borders
    "editorGroup.border": "#282828",
    "editorHint.border": "#282828",
    "tab.border": "#282828",
    "menu.border": "#282828",
    "focusBorder": "#282828",
    "input.border": "#282828",
    "merge.border": "#282828",
    "panel.border": "#282828",
    "button.border": "#282828",
    "widget.border": "#282828",
    "sideBar.border": "#282828",
    "contrastBorder": "#282828",
    "checkbox.border": "#282828",
    "peekView.border": "#282828",
    "dropdown.border": "#282828",
    "tab.hoverBorder": "#282828",
    "terminal.border": "#282828",
    "titleBar.border": "#282828",
    "statusBar.border": "#282828",

    //Terminal color
    "terminal.background": "#0f1014",
    "terminal.foreground": "#B3B1AD",
    "terminal.ansiBlack": "#6F6F6F",
    "terminal.ansiBlue": "#b6b1d2",
    "terminal.ansiCyan": "#7FC9D6",
    "terminal.ansiGreen": "#9168cb",
    "terminal.ansiMagenta": "#C7C7C7",
    "terminal.ansiRed": "#F8B8AB",
    "terminal.ansiWhite": "#C2BBD3",
    "terminal.ansiYellow": "#A88BFA",
    "terminal.ansiBrightBlack": "#01060E",
    "terminal.ansiBrightBlue": "#C2BBD3",
    "terminal.ansiBrightCyan": "#a1d3e0",
    "terminal.ansiBrightGreen": "#ae89ea",
    "terminal.ansiBrightMagenta": "#a783f4",
    "terminal.ansiBrightRed": "#F8B8AB",
    "terminal.ansiBrightWhite": "#A88BFA",
    "terminal.ansiBrightYellow": "#835ee3",
    // Editor Line numbers
    "editorLineNumber.foreground": "#4d50578e",

    // Editor indent Guide
    "editorBracketPairGuide.background1": "#363536",
    "editorBracketPairGuide.background2": "#363536",
    "editorBracketPairGuide.background3": "#363536",
    "editorBracketPairGuide.background4": "#363536",
    "editorBracketPairGuide.background5": "#363536",
    "editorBracketPairGuide.background6": "#363536",

    // Tree indent Guide
    "tree.indentGuidesStroke": "#363536"
